<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Project" table="project">
        <id name="projectID" type="java.lang.Integer">
            <column name="project_id" />
            <generator class="identity" />
        </id>
        <property name="title" type="string">
            <column name="title" length="255" not-null="true" />
        </property>
        <set name="employees" table="empl_project"
             inverse="false" lazy="true" fetch="select" cascade="all" >
            <key>
                <column name="proj_id" not-null="true" />
            </key>
            <many-to-many entity-name="entity.Employee">
                <column name="empl_id" not-null="true" />
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>